<%= form_for [current_user, @post] do |f| %>

  <div class="field">
		<%= f.label :title %><br />
		<%= f.text_field :title %>
	</div>

	<div class="field">
		<%= f.label :content %><br />
		<%= f.text_area :content %>
	</div>

	<div class="field">
		<%= f.label :images %><br />
		<%= f.file_field :images %>
	</div>

  <%= f.submit "Submit", id: "submit" %>
<% end %>
  
<%= link_to 'Cancel', root_path %>